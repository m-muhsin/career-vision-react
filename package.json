{
  "name": "career-vision-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run setup-api",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
